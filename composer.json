{
    "name": "winternet-studio/yii2-wordpress-glue",
    "description": "Toolkit for Wordpress-Plugin Development with yii2-framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Allan Jensen",
            "email": "yii2-libs@allanid.com"
        },
        {
            "name": "Henry Volkmer",
            "email": "henry.volkmer@mailbox.org"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "winternet\\yii2wordpress\\": "src/"
        }
    },
    "bin": ["src/bin/yii2wp"],
    "require": {
        "php": ">=7.3",
        "yiisoft/yii2": ">=2.0.25 <2.1",
        "yiisoft/yii2-bootstrap4": "^2.0",
        "kartik-v/yii2-detail-view": "1.8.x-dev",
        "yiisoft/yii2-httpclient": "^2.0@dev",
        "kartik-v/yii2-widget-select2": "^2.1@dev",
        "kartik-v/yii2-date-range": "1.7.x-dev",
        "symfony/dotenv": "^5.2"
    }
}
